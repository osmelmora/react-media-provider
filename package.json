{
  "name": "react-mq-provider",
  "version": "1.0.0",
  "description": "Application wide media queries",
  "author": "Osmel Mora",
  "license": "MIT",
  "repository": "osmelmora/react-mq-provider",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "lint": "eslint src",
    "lint:staged": "lint-staged",
    "precommit": "npm run lint:staged",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && yarn install && yarn build",
    "deploy": "gh-pages -d example/build"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "*.js": "lint"
  },
  "dependencies": {},
  "peerDependencies": {
    "create-react-context": "^0.2.2",
    "create-subscription": "^16.3.2",
    "fbjs": "^0.8.16",
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-lifecycles-compat": "^3.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "create-react-context": "^0.2.2",
    "create-subscription": "^16.3.2",
    "cross-env": "^5.1.4",
    "dom-testing-library": "^2.3.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "fbjs": "^0.8.16",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-lifecycles-compat": "^3.0.3",
    "react-scripts": "^1.1.1",
    "react-testing-library": "^3.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "rollup-plugin-url": "^1.3.0"
  },
  "files": ["dist"]
}
